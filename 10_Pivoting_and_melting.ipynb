    "import pandas as pd\n",
    "\n",
    "dict = {\"keys\":[\"k1\",\"k2\",\"k1\",\"k2\"],\n",
    "        \"names\":[\"John\",\"Ben\",\"David\",\"Peter\"],\n",
    "        \"houses\":[\"red\",\"blue\",\"green\",\"red\"]}\n",
    "\n",
    "df = pd.DataFrame(dict)\n",
    "print(df)"
   ]
